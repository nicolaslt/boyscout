{
  "name": "eslint-boyscout",
  "version": "0.1.3",
  "description": "A set of tools to help keep track of long winded refactoring.",
  "main": "index.js",
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.18.0",
    "eslint-plugin-boyscout": "^0.1.0",
    "pluralize": "^7.0.0"
  },
  "devDependencies": {
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier-eslint-cli": "^4.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.js": ["prettier-eslint", "git add"]
  },
  "scripts": {
    "test": "mocha",
    "format": "prettier-eslint --write \"bin/**/*.js\" \"lib/**/*.js\"",
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicolaslt/eslint-boyscout.git"
  },
  "keywords": [
    "eslint",
    "boyscout"
  ],
  "author": "Anonymous Coward",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicolaslt/eslint-boyscout/issues"
  },
  "homepage": "https://github.com/nicolaslt/eslint-boyscout#readme",
  "bin": {
    "boyscout": "bin/boyscout.js"
  }
}
